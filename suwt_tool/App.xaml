<Application x:Class="suwt.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <sys:Double x:Key="IntervalTimerVol">50</sys:Double>
        <sys:Double x:Key="LevelVolUp">100</sys:Double>
        <sys:Double x:Key="LevelVolDown">25</sys:Double>
        <sys:Double x:Key="LevelVolStep">4</sys:Double>

        <sys:Double x:Key="LevelMicUp">-20</sys:Double>
        <sys:Double x:Key="LevelMicDown">-30</sys:Double>
    </Application.Resources>
</Application>
